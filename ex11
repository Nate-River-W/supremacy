#include <stdio.h>
char* my_capitalize(char* param_1)
{	int i = 0;
	while(param_1[i] != '\0')
	{	if(i = 0 && param_1[i] >= 97 && param_1[i] <= 122)
			{param_1[i] -=  32;
		
			 }
		else if(i != 0 && param_1[i] >= 65 && param_1[i] <= 90 )
			{param_1[i] +=  32;	
			}
		
		i++;	
	}
 	return param_1;
	
}	
int main(){
	printf("%s",my_capitalize("abC"));

	return 0;
}
